{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  },
  "SwitchBotOption": {
    "Token": "YourSwitchBotToken",
    "Secret": "YourSwitchBotSecret"
  },
  "InfluxDbOption": {
    "Url": "http://localhost:8086",
    "Token": "Token",
    "Bucket": "Bucket",
    "Org": "Org"
  },
  "Target": [
    {
      "DeviceId": "DeviceId",
      "Measurement": "SwitchBot",
      "Keys": [
        "battery",
        "temperature",
        "humidity",
        "CO2"
      ],
      "Tags": [
        {
          "Key": "Room",
          "Value": "LivingRoom"
        }
      ],
      "ApiInterval": 60
    }
  ]
}
